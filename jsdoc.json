{
	"plugins": ["plugins/markdown"],
	"source": {
		"include": ["./src", "lib", "package.json", "README.md"],
		"includePattern": ".+\\js(doc|x)?$",
		"exclude": "(node_modules/|docs)"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"encoding": "utf8",
		"readme": "./README.md",
		"destination": "docs/",
		"recurse": true,
		"verbose": true,
		"template": "./node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"default_theme": "fallback-light",
      "homepageTitle": "PRAE Front Docs",
